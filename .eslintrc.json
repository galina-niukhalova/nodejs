{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "allowImportExportEverywhere": true
    },
    "rules": {
        "semi": "error",
        "func-names": [
            "error",
            "never"
        ]
    },
    "extends": "airbnb"
}